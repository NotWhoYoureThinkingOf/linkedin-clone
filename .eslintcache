rorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"36"},{"filePath":"67","messages":"68","errorCount":0,"warningCount":4,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},{"filePath":"69","messages":"70","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"36"},"/mnt/e/projects/linkedin-clone/src/index.js",[],["71","72"],"/mnt/e/projects/linkedin-clone/src/App.js",["73"],"import React, { useEffect } from \"react\";\r\nimport { useDispatch, useSelector } from \"react-redux\";\r\nimport \"./App.css\";\r\nimport Feed from \"./components/Feed\";\r\nimport Header from \"./components/Header\";\r\nimport Login from \"./components/Login\";\r\nimport Sidebar from \"./components/Sidebar\";\r\nimport Widgets from \"./components/Widgets\";\r\nimport { selectUser, logout, login } from \"./features/user/userSlice\";\r\nimport { auth } from \"./firebase\";\r\n\r\nfunction App() {\r\n  const user = useSelector(selectUser);\r\n  const dispatch = useDispatch();\r\n\r\n  useEffect(() => {\r\n    // authentication persistence, will keep user logged in unless the state of the authentication has changed\r\n    auth.onAuthStateChanged((userAuth) => {\r\n      if (userAuth) {\r\n        dispatch(\r\n          login({\r\n            email: userAuth.email,\r\n            uid: userAuth.uid,\r\n            displayName: userAuth.displayName,\r\n            photoUrl: userAuth.photoURL,\r\n          })\r\n        );\r\n      } else {\r\n        dispatch(logout());\r\n      }\r\n    });\r\n  }, []);\r\n\r\n  return (\r\n    <div className=\"app\">\r\n      <Header />\r\n      {!user ? (\r\n        <Login />\r\n      ) : (\r\n        <div className=\"app__body\">\r\n          <Sidebar />\r\n          <Feed />\r\n          <Widgets />\r\n        </div>\r\n      )}\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","/mnt/e/projects/linkedin-clone/src/serviceWorker.js",[],"/mnt/e/projects/linkedin-clone/src/app/store.js",[],"/mnt/e/projects/linkedin-clone/src/features/counter/counterSlice.js",[],"/mnt/e/projects/linkedin-clone/src/features/counter/Counter.js",[],"/mnt/e/projects/linkedin-clone/src/components/Header.js",["74","75","76","77"],"import React from \"react\";\r\nimport \"./Header.css\";\r\nimport {\r\n  Search,\r\n  Home,\r\n  SupervisorAccount,\r\n  BusinessCenter,\r\n  Chat,\r\n  Notifications,\r\n} from \"@material-ui/icons\";\r\nimport HeaderOption from \"./HeaderOption\";\r\nimport { Avatar } from \"@material-ui/core\";\r\nimport { useDispatch, useSelector } from \"react-redux\";\r\nimport { logout, selectUser } from \"../features/user/userSlice\";\r\nimport { auth } from \"../firebase\";\r\n\r\nconst Header = () => {\r\n  const dispatch = useDispatch();\r\n\r\n  const logoutOfApp = () => {\r\n    dispatch(logout());\r\n    auth.signOut();\r\n  };\r\n\r\n  return (\r\n    <div className=\"header\">\r\n      <div className=\"header__left\">\r\n        <img src=\"https://image.flaticon.com/icons/png/512/174/174857.png\"></img>\r\n        <div className=\"header__search\">\r\n          <Search />\r\n          <input placeholder=\"Search\" type=\"text\" />\r\n        </div>\r\n      </div>\r\n      <div className=\"header__right\">\r\n        <HeaderOption Icon={Home} title=\"Home\" />\r\n        <HeaderOption Icon={SupervisorAccount} title=\"My Network\" />\r\n        <HeaderOption Icon={BusinessCenter} title=\"Jobs\" />\r\n        <HeaderOption Icon={Chat} title=\"Messaging\" />\r\n        <HeaderOption Icon={Notifications} title=\"Notifications\" />\r\n        <HeaderOption avatar={true} title=\"Me\" onClick={logoutOfApp} />\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Header;\r\n","/mnt/e/projects/linkedin-clone/src/components/HeaderOption.js",[],"/mnt/e/projects/linkedin-clone/src/components/Sidebar.js",[],"/mnt/e/projects/linkedin-clone/src/components/Feed.js",["78"],"import {\r\n  CalendarViewDay,\r\n  Create,\